local Assail = game.Players.LocalPlayer.Backpack.Assailant
local Cool = Assail.Cooldowns
local Char = game.Players.LocalPlayer.Character
local UserInputService = game:GetService("UserInputService")

UserInputService.InputBegan:connect(function(inst)
        if inst.KeyCode == Enum.KeyCode.Q and Cool.Guardbreak.Value == 0 and Char.Conditions.Heat.Value == 0 < 200 then
            AnimationId = "5779600544"
            local Anim = Instance.new("Animation")
            Anim.AnimationId = "rbxassetid://" .. AnimationId
            local k = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(Anim)
            wait(0.1)
            k:Play()
            k:AdjustSpeed(1)
        end
    end
)

UserInputService.InputBegan:connect(function(inst)
        if inst.KeyCode == Enum.KeyCode.One and Cool["1"].Value == 0 then
            AnimationId = "3372690528"
            local Anim = Instance.new("Animation")
            Anim.AnimationId = "rbxassetid://" .. AnimationId
            local k = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(Anim)
            wait(0.1)
            k:Play()
            k:AdjustSpeed(1)
        end
    end
)

