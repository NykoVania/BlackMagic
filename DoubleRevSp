local uis = game:GetService("UserInputService")
local Player = game.Players.LocalPlayer
local Special1
local Special2
local Special6
local Special6
local Special3
local Special7
local Special5

uis.InputEnded:connect(
    function(input)
        if input.KeyCode == Enum.KeyCode.One then
            Special1 = false
        end
    end
)

uis.InputBegan:connect(
    function(input)
        if input.KeyCode == Enum.KeyCode.One then
            Special1 = true
        end
    end
)

uis.InputEnded:connect(
    function(input)
        if input.KeyCode == Enum.KeyCode.Six then
            Special6 = false
        end
    end
)

uis.InputBegan:connect(
    function(input)
        if input.KeyCode == Enum.KeyCode.Six then
            Special6 = true
        end
    end
)
uis.InputEnded:connect(
    function(input)
        if input.KeyCode == Enum.KeyCode.Three then
            Special3 = false
        end
    end
)

uis.InputBegan:connect(
    function(input)
        if input.KeyCode == Enum.KeyCode.Three then
            Special3 = true
        end
    end
)
uis.InputEnded:connect(
    function(input)
        if input.KeyCode == Enum.KeyCode.Two then
            Special2 = false
        end
    end
)

uis.InputBegan:connect(
    function(input)
        if input.KeyCode == Enum.KeyCode.Two then
            Special2 = true
        end
    end
)
uis.InputEnded:connect(
    function(input)
        if input.KeyCode == Enum.KeyCode.Seven then
            Special7 = false
        end
    end
)

uis.InputBegan:connect(
    function(input)
        if input.KeyCode == Enum.KeyCode.Seven then
            Special7 = true
        end
    end
)
uis.InputEnded:connect(
    function(input)
        if input.KeyCode == Enum.KeyCode.Five then
            Special5 = false
        end
    end
)

uis.InputBegan:connect(
    function(input)
        if input.KeyCode == Enum.KeyCode.Five then
            Special5 = true
        end
    end
)
while wait() do
    if Special1 == true then
        game:GetService("Players").LocalPlayer.Backpack.Revenant.Signals.SendData:FireServer("DoMove", "SP1", false)
    elseif Special2 == true then
        game:GetService("Players").LocalPlayer.Backpack.Revenant.Signals.SendData:FireServer("DoMove", "SP2", false)
    elseif Special7 == true then
        game:GetService("Players").LocalPlayer.Backpack.Revenant.Signals.SendData:FireServer("DoMove", "SP7", false)
    elseif Special6 == true then
        game:GetService("Players").LocalPlayer.Backpack.Revenant.Signals.SendData:FireServer("DoMove", "SP6", false)
    elseif Special3 == true then
        game:GetService("Players").LocalPlayer.Backpack.Revenant.Signals.SendData:FireServer("DoMove", "SP3", false)
    elseif Special5 == true then
        game:GetService("Players").LocalPlayer.Backpack.Revenant.Signals.SendData:FireServer("DoMove", "SP5", false)
    end
end
