-- By Nyko#8409 :)
local UserInputService = game:GetService('UserInputService')
local DoubleDebounce = false 
local TargetKeyCode = Enum.KeyCode.D 

UserInputService.InputBegan:Connect(function(input,gameEvent)
    if not gameEvent and input.KeyCode == TargetKeyCode then
        if not DoubleDebounce then
            DoubleDebounce = true
            spawn(function()
                wait(.2)
                DoubleDebounce = false
            end)
        elseif DoubleDebounce then
            game:GetService("Players").LocalPlayer.Backpack.Revenant.Signals.SendData:FireServer("DoMove","Dodge")
            DoubleDebounce = false
        end
    end
end)
local DoubleDebounce2 = false 
local TargetKeyCode2 = Enum.KeyCode.S

UserInputService.InputBegan:Connect(function(input,gameEvent)
    if not gameEvent and input.KeyCode == TargetKeyCode2 then
        if not DoubleDebounce2 then
            DoubleDebounce2 = true
            spawn(function()
                wait(.2)
                DoubleDebounce2 = false
            end)
        elseif DoubleDebounce2 then
            game:GetService("Players").LocalPlayer.Backpack.Revenant.Signals.SendData:FireServer("DoMove","Backhop")
            DoubleDebounce2 = false
        end
    end 
end)
